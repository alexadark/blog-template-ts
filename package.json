    "@storyblok/react": "^2.0.17",
    "@vercel/node": "^2.6.2",
    "date-fns": "^2.29.3",
    "isbot": "^3.6.5",
    "postcss-import": "^15.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-headroom": "^3.2.1",
